{
  "daemon": "onemetre_telescope",
  "log_name": "teld",
  "control_machines": ["OneMetreDome", "OneMetreTCS", "OneMetreDAS"],
  "security_system_daemon": "onemetre_domealert",
  "security_system_key": "security_system_safe",
  "telescope": "W1m",
  "query_delay": 1,
  "query_timeout_iterations": 30,
  "focus_tolerance": 0.005,
  "initialization_timeout": 60,
  "slew_timeout": 120,
  "focus_timeout": 60,
  "homing_timeout": 120,
  "limit_timeout": 120,
  "ping_timeout": 5,
  "ha_soft_limits": [-72, 90],
  "dec_soft_limits": [-30, 85],
  "park_positions": {
    "stow": {
      "desc": "default park position",
      "ha_enc": 2300000,
      "dec_enc": -2500000
    },
    "zenith": {
      "desc": "park pointing straight up",
      "ha_enc": -534000,
      "dec_enc": -593400
    }
  }
}
