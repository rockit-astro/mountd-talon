{
  "daemon": "superwasp_telescope",
  "log_name": "superwasp_teld",
  "control_machines": ["SWASPTCS"],
  "telescope": "SuperWASP",
  "virtual": false,
  "query_delay": 1,
  "focus_tolerance": 0.005,
  "initialization_timeout": 60,
  "slew_timeout": 120,
  "focus_timeout": 60,
  "homing_timeout": 120,
  "limit_timeout": 120,
  "cover_timeout": 2.5,
  "ping_timeout": 5,
  "ha_soft_limits": [-72, 90],
  "dec_soft_limits": [-30, 85],
  "roof_open_timeout": 60,
  "roof_close_timeout": 120,
  "park_positions": {
    "zenith": {
      "desc": "park at zenith",
      "alt": 89.9,
      "az": 0
    }
  }
}
