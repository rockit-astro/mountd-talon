{
  "daemon": "ngts_m06_telescope",
  "log_name": "teld@ngts_m06",
  "control_machines": ["NGTSDASNUC"],
  "telescope": "NGTS",
  "query_delay": 1,
  "initialization_timeout": 60,
  "slew_timeout": 120,
  "homing_timeout": 120,
  "limit_timeout": 120,
  "ping_timeout": 5,
  "ha_soft_limits": [-75, 75],
  "dec_soft_limits": [-89, 30],
  "park_positions": {
    "stow": {
      "desc": "park flat protecting camera and optics",
      "ha_enc": 0,
      "dec_enc": -10475000
    }
  }
}
